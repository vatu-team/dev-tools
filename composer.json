{
    "name": "vatu/dev-tools",
    "description": "A collection of development tools use in Vatu projects.",
    "type": "metapackage",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "brain/monkey": "2.6.1",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "phpunit/phpunit": "9.5.27",
        "povils/phpmnd": "3.0.1",
        "sebastian/phpcpd": "6.0.3",
        "yoast/phpunit-polyfills": "1.0.4"
    },
    "config": {
      "sort-packages": true,
      "optimize-autoloader": true,
      "preferred-install": "dist"
    },
    "prefer-stable": true
}
